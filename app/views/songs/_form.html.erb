
<%= form_for @song do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
<p>
  <%= f.label :genre %>
  <%= f.text_field :genre %>
  </p>

<p>
  <%= f.label :released %>
  <%= f.check_box :released %>
  </p>

<p>
  <%= f.label :release_year %>
  <%= f.number_field :release_year %>
  </p>

<p>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  </p>
  
  <%= f.submit %>
<% end %>
